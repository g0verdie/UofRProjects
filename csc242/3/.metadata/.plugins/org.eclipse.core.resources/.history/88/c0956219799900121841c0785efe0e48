import java.util.*;
public class ExactInferenceRG {
	
	RandomVariableRG query;
	ArrayList<RandomVariableRG> evidence;
	BayesianNetworkRG bnet;
	
	public ExactInference(RandomVariableRG query,ArrayList<RandomVariableRG> evidence,BayesianNetworkRG bnet) {
		this.query = query;
		this.evidence = evidence;
		this.bnet = bnet;
	}

}
