int strcmp2(char *s, char *t)
{
	for(; *s == *t; s++,t++)
	{
		if(*s == '\0')
			return 0;
	}
	return *s - *t;

}

int strcat2(char *s, char *t)
{
	int shold = s;
	while(*s != '\0')
	{
		s++;
	}
	while(*t != '\0')
	{
		*s = *t;
		//printf("%c",*s);
		s++;
		t++;
	}
	int i = shold;
	while(i != shold+6)
		{
			printf("%c",i);
			printf("hi");
			i++;
		}
	return 1;
}

